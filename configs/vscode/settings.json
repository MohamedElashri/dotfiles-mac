{
  "CodeGPT.Autocomplete.maxTokens": 5000,
  "CodeGPT.apiKey": "Google AI Studio",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "amazonQ.shareContentWithAWS": false,
  "amazonQ.telemetry": false,
  "application.shellEnvironmentResolutionTimeout": 20,
  "aws.codeWhisperer.shareCodeWhispererContentWithAWS": false,
  "aws.telemetry": false,
  "codeium.enableConfig": {
    "*": false,
    "c": false,
    "python": false
  },
  "cody.plugins.debug.enabled": false,
  "continue.enableTabAutocomplete": true,
  "editor.accessibilitySupport": "off",
  "editor.defaultFormatter": "ms-python.autopep8",
  "editor.fontFamily": "Monaspace Argon",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.fontWeight": "bold",
  "editor.formatOnPaste": true,
  "editor.inlineSuggest.enabled": true,
  "editor.inlineSuggest.showToolbar": "always",
  "editor.minimap.autohide": true,
  "editor.minimap.enabled": false,
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "git.autofetch": true,
  "git.ignoreLegacyWarning": true,
  "github.copilot-labs.showBrushesLenses": true,
  "github.copilot-labs.showTestGenerationLenses": true,
  "github.copilot.advanced": {},
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": false,
    "python": true,
    "quarto": false,
    "scminput": false
  },
  "gitlens.advanced.messages": {
    "suppressGitVersionWarning": true
  },
  "gitlens.ai.experimental.anthropic.model": "claude-instant-1",
  "gitlens.ai.experimental.provider": "anthropic",
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "stash-push:command",
    "switch:command"
  ],
  "gitlens.graph.layout": "editor",
  "gitlens.views.remotes.branches.layout": "list",
  "jupyter.askForKernelRestart": false,
  "prettier.bracketSameLine": true,
  "python.formatting.provider": "none",
  "redhat.telemetry.enabled": false,
  "remote.SSH.connectTimeout": 100,
  "remote.SSH.remotePlatform": {
    "coder-vscode--melashri--VisualStudio": "linux",
    "lxplus": "linux",
    "sleepy-earth": "linux",
    "sneezy-earth": "linux"
  },
  "remoteHub.commitDirectlyWarning": "off",
  "rootFileViewer.darkMode": true,
  "semanticdiff.closeOriginalTab": true,
  "semanticdiff.defaultDiffViewer": true,
  "terminal.external.osxExec": "Warp.app",
  "terminal.integrated.env.osx": {
    "CW_NEW_SESSION": "1",
    "FIG_NEW_SESSION": "1",
    "Q_NEW_SESSION": "1"
  },
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "terminal.integrated.persistentSessionScrollback": 1000,
  "terminal.integrated.scrollback": 10000,
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.colorCustomizations": {
    "terminal.ansiBlack": "#191919",
    "terminal.ansiBlue": "#639EE4",
    "terminal.ansiBrightBlack": "#5F5A60",
    "terminal.ansiBrightBlue": "#639EE4",
    "terminal.ansiBrightCyan": "#4BB1A7",
    "terminal.ansiBrightGreen": "#84B97C",
    "terminal.ansiBrightMagenta": "#B888E2",
    "terminal.ansiBrightRed": "#DC657D",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBrightYellow": "#C3BA63",
    "terminal.ansiCyan": "#4BB1A7",
    "terminal.ansiGreen": "#84B97C",
    "terminal.ansiMagenta": "#B888E2",
    "terminal.ansiRed": "#DC657D",
    "terminal.ansiWhite": "#A7A7A7",
    "terminal.ansiYellow": "#C3BA63",
    "terminal.background": "#191919",
    "terminal.foreground": "#A7A7A7",
    "terminalCursor.background": "#A7A7A7",
    "terminalCursor.foreground": "#A7A7A7"
  },
  "workbench.colorTheme": "Dracula Pro",
  "workbench.editorAssociations": {
    "*.xls": "default",
    "*.ipynb": "default"
  },
  "workbench.editorLargeFileConfirmation": 50,
  "workbench.iconTheme": "vscode-icons",
  "jupyter.allowUnauthorizedRemoteConnection": true,
  "jupyter.executionAnalysis.enabled": true,
  "jupyter.showVariableViewWhenDebugging": true,
  "jupyter.themeMatplotlibPlots": true,
  "notebook.defaultFormatter": "esbenp.prettier-vscode",
  "jupyter.completionTriggerCharacters": {
    "bash": [
      "$",
      "{"
    ],
    "java": [
      ".",
      "@",
      "#"
    ],
    "javascript": [
      ".",
      "\"",
      "'",
      "/",
      "@"
    ],
    "python": [
      ".",
      "%",
      "'",
      "\""
    ],
    "julia": [
      "."
    ],
    "r": [
      "[",
      "(",
      ",",
      "$",
      "@",
      "\"",
      "'"
    ],
    "rust": [
      ".",
      ":"
    ],
    "scala": [
      "."
    ],
    "typescript": [
      ".",
      "\"",
      "'",
      "/",
      "@",
      "<"
    ]
  },
  "cmake.showOptionsMovedNotification": false,
  "remote.tunnels.alwaysUpdateCLI": true
}
